<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.a0xmati.kodzik">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".Activites.MainActivity"
            android:label="@string/label1">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".Activites.GameActivity"
            android:label="@string/label2"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activites.SettingsActivity"
            android:label="@string/label3" />
        <activity
            android:name=".Activites.FavouritesActivity"
            android:label="@string/title_dashboard"></activity>
    </application>

</manifest>